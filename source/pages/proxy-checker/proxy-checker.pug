									label.check.noselect
										input(type='checkbox')
										i.fa.fa-1.fa-check-square
										i.fa.fa-2.fa-square-o
										span HTTP | HTTPS
									label.check.noselect
										input(type='checkbox')
										i.fa.fa-1.fa-check-square
										i.fa.fa-2.fa-square-o
										span SOCKS 4
									label.check.noselect
										input(type='checkbox')
										i.fa.fa-1.fa-check-square
										i.fa.fa-2.fa-square-o
										span SOCKS 5
									label.check.noselect
										input(type='checkbox')
										i.fa.fa-1.fa-check-square
										i.fa.fa-2.fa-square-o
										span Нерабочие
									label.check.noselect
